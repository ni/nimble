{
  "name": "@ni-private/performance",
  "version": "1.0.0",
  "private": true,
  "description": "A package to measure the performance of the Nimble Components",
  "scripts": {
    "build": "tsc && vite build src",
    "pack": "npm pack",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "start": "vite src",
    "performance": "lhci autorun"
  },
  "type": "module",
  "files": [
    "dist",
    "!*.tgz"
  ],
  "dependencies": {
    "@ni/nimble-components": "*"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.0",
    "@ni-private/eslint-config-nimble": "*",
    "typescript": "~5.8.3",
    "vite": "^7.0.0"
  }
}
