{
  "name": "@ni/nimble-react",
  "version": "0.10.4",
  "description": "React components for the NI Nimble Design System",
  "keywords": [
    "ni",
    "nimble",
    "react"
  ],
  "scripts": {
    "build": "npm run build:icons && npm run build:components",
    "build:icons": "node build/generate-icons.mjs",
    "build:components": "tsc -p ./tsconfig.json",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "pack": "npm pack",
    "invoke-publish": "npm publish"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./styles/*": "./styles/*",
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/*",
        "dist/esm/*/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ni/nimble.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "National Instruments",
  "license": "MIT",
  "peerDependencies": {
    "@lit/react": "^1.0.8",
    "@ni/nimble-components": "^35.0.2",
    "react": "^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@ni-private/eslint-config-nimble": "*",
    "@ni/fast-web-utilities": "^10.0.0",
    "@ni/nimble-components": "^35.0.2",
    "@ni/nimble-tokens": "^8.13.7",
    "typescript": "~5.8.3"
  }
}
