{
  "name": "@ni/nimble-react",
  "version": "0.8.2",
  "description": "React components for the NI Nimble Design System",
  "keywords": [
    "ni",
    "nimble",
    "react"
  ],
  "type": "module",
  "exports": {
      "./styles/*": "./styles/*",
      "./icons/*": {
        "types": "./dist/esm/icons/*.d.ts",
        "import": "./dist/esm/icons/*.js"
      },
      "./*": {
        "types": "./dist/esm/*.d.ts",
        "import": "./dist/esm/*/index.js"
      }
  },
  "typesVersions": {
    "*": {
      "*": ["dist/esm/*"]
    }
  },
  "scripts": {
    "build": "npm run build:icons && npm run build:components",
    "build:icons": "node build/generate-icons.mjs",
    "build:components": "tsc -p ./tsconfig.json",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "pack": "npm pack",
    "invoke-publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ni/nimble.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "National Instruments",
  "license": "MIT",
  "devDependencies": {
    "@ni-private/eslint-config-nimble": "*",
    "@ni/fast-web-utilities": "^10.0.0",
    "@ni/nimble-components": "^34.10.3",
    "@ni/nimble-tokens": "^8.13.6",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "@lit/react": "^1.0.8",
    "@ni/nimble-components": "^34.10.3",
    "react": "^17 || ^18 || ^19"
  }
}
