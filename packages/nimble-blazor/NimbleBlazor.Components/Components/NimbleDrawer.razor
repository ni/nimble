@namespace NimbleBlazor.Components
<nimble-drawer
        location="@DrawerLocationExtensions.ToAttributeValue(Location)"
        state="@DrawerStateExtensions.ToAttributeValue(State)"
        @onnimbledrawerstatechange="(__value) => UpdateDrawerState(__value.DrawerState)"
        modal="@Modal"
        prevent-dismiss="@PreventDismiss"
        @attributes="AdditionalAttributes">
    @ChildContent
</nimble-drawer>