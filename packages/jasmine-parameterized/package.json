{
    "name": "@ni/jasmine-parameterized",
    "version": "1.0.0",
    "description": "A utility to write parameterized jasmine tests",
    "scripts": {
        "build": "npm run compile",
        "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json",
        "lint": "eslint .",
        "format": "eslint . --fix",
        "pack": "npm pack",
        "invoke-publish": "npm publish",
        "tdd": "npm run build && npm run test",
        "test": "karma start karma.conf.js --browsers=ChromeHeadlessOpt --single-run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ni/nimble.git",
        "directory": "packages/jasmine-parameterized"
    },
    "main": "./dist/esm/parameterized.js",
    "types": "./dist/esm/parameterized.d.ts",
    "type": "module",
    "author": "National Instruments",
    "license": "MIT",
    "devDependencies": {
        "@ni/eslint-config-javascript": "^4.2.0",
        "@ni/eslint-config-typescript": "^4.2.0",
        "@types/jasmine": "^4.3.1",
        "@types/yargs": "^17.0.10",
        "jasmine": "^4.2.0",
        "jasmine-core": "^4.5.0",
        "karma": "^6.3.0",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "karma-source-map-support": "^1.4.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "^0.0.36",
        "karma-webpack": "^5.0.0",
        "playwright": "^1.30.0",
        "source-map-loader": "^4.0.1",
        "typescript": "~4.8.2"
    }
}
