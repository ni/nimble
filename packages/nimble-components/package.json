{
  "name": "@ni/nimble-components",
  "version": "35.3.0",
  "description": "Styled web components for the NI Nimble Design System",
  "scripts": {
    "build": "npm run build:icons && npm run build:workers && npm run build:components && npm run build:scss",
    "build-bundle": "rollup --config",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "pack": "npm pack",
    "invoke-publish": "npm publish",
    "build:components": "tsc -p ./tsconfig.json",
    "build:icons": "node build/generate-icons.mjs",
    "build:scss": "node build/generate-scss.mjs",
    "build:workers": "npm run build:workers:compile && npm run build:workers:bundle && npm run build:workers:inline",
    "build:workers:compile": "tsc -p build/generate-workers/tsconfig.json",
    "build:workers:bundle": "rollup --config build/generate-workers/rollup.config.mjs",
    "build:workers:inline": "node build/generate-workers.mjs",
    "test-chrome-debugger": "karma start karma.conf.cjs --browsers=ChromeDebugging --skip-tags SkipChrome",
    "test-chrome": "karma start karma.conf.headless.cjs --browsers=ChromeHeadlessOpt --single-run --skip-tags SkipChrome",
    "test-firefox-debugger": "karma start karma.conf.cjs --browsers=FirefoxDebugging --skip-tags SkipFirefox",
    "test-firefox": "karma start karma.conf.headless.cjs --browsers=FirefoxHeadless --single-run --skip-tags SkipFirefox",
    "test-webkit-debugger": "karma start karma.conf.cjs --browsers=WebkitDebugging --skip-tags SkipWebkit",
    "test-webkit": "karma start karma.conf.headless.cjs --browsers=WebkitHeadless --single-run --skip-tags SkipWebkit",
    "test": "concurrently --pad-prefix --timings --group \"npm:test-chrome\" \"npm:test-webkit\""
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ni/nimble.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "National Instruments"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ni/nimble/issues"
  },
  "homepage": "https://github.com/ni/nimble#readme",
  "dependencies": {
    "@ni/fast-colors": "^10.0.0",
    "@ni/fast-element": "^10.1.0",
    "@ni/fast-foundation": "^10.1.5",
    "@ni/fast-web-utilities": "^10.0.0",
    "@ni/nimble-tokens": "^8.15.0",
    "@ni/unit-format": "^1.0.2",
    "@tanstack/table-core": "^8.19.3",
    "@tanstack/virtual-core": "^3.10.6",
    "@tiptap/core": "^3.10.1",
    "@tiptap/extension-bold": "^3.10.1",
    "@tiptap/extension-document": "^3.10.1",
    "@tiptap/extension-hard-break": "^3.10.1",
    "@tiptap/extension-italic": "^3.10.1",
    "@tiptap/extension-link": "^3.10.1",
    "@tiptap/extension-list": "^3.10.1",
    "@tiptap/extension-mention": "^3.10.1",
    "@tiptap/extension-paragraph": "^3.10.1",
    "@tiptap/extension-text": "^3.10.1",
    "@tiptap/extensions": "^3.10.1",
    "@tiptap/suggestion": "^3.10.1",
    "@types/d3-array": "^3.0.4",
    "@types/d3-random": "^3.0.1",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-selection": "^3.0.0",
    "@types/d3-zoom": "^3.0.0",
    "@types/markdown-it": "^14.1.2",
    "comlink": "4.4.2",
    "d3-array": "^3.2.2",
    "d3-random": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "prosemirror-markdown": "^1.13.1",
    "prosemirror-model": "^1.24.0",
    "prosemirror-state": "^1.4.3",
    "tslib": "^2.2.0"
  },
  "peerDependencies": {
    "apache-arrow": "21.1.0"
  },
  "devDependencies": {
    "@ni-private/eslint-config-nimble": "*",
    "@ni-private/jasmine-extensions": "^0.0.1",
    "@ni/jasmine-parameterized": "^1.0.9",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/jasmine": "^5.1.4",
    "@types/offscreencanvas": "^2019.7.3",
    "concurrently": "^9.0.0",
    "jasmine-core": "^5.1.2",
    "karma": "^6.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-jasmine-spec-tags": "^2.0.0",
    "karma-spec-reporter": "^0.0.36",
    "karma-vite": "^1.0.5",
    "karma-webkit-launcher": "^2.6.0",
    "playwright": "1.54.1",
    "rollup": "^4.12.0",
    "rollup-plugin-sourcemaps2": "^0.5.0",
    "typescript": "~5.8.3"
  }
}
